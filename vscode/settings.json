{
  "breadcrumbs.symbolPath": "off",
  "diffEditor.renderSideBySide": false,
  "editor.autoClosingQuotes": "never",
  "editor.autoClosingBrackets": "never",
  "editor.autoIndent": "advanced",
  "editor.autoSurround": "never",
  "editor.folding": false,
  "editor.foldingHighlight": false,
  "editor.fontFamily": "'Cascadia Mono', 'Ubuntu Mono', 'Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
  "editor.lightbulb.enabled": false,
  "editor.minimap.enabled": false,
  "editor.minimap.showSlider": "always",
  "editor.occurrencesHighlight": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": false
  },
  "editor.renderLineHighlight": "none",
  "editor.scrollBeyondLastLine": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.tabSize": 2,
  "elixirLS.suggestSpecs": false,
  "evenBetterToml.formatter.trailingNewline": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "languageToolLinter.languageTool.motherTongue": "fr",
  "magit.display-buffer-function": "same-column",
  "projectManager.git.baseFolders": [
    "~/Code"
  ],
  "scm.diffDecorationsGutterWidth": 3,
  "search.seedOnFocus": true,
  "tailwindCSS.includeLanguages": {
    "html-eex": "html",
    "phoenix-heex": "html"
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "update.channel": "none",
  "window.title": "[Code] ${rootName}",
  "window.titleBarStyle": "native",
  "workbench.statusBar.feedback.visible": false,
  "workbench.colorTheme": "One Dark Pro Flat",
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.titleScrollbarSizing": "large",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.startupEditor": "newUntitledFile",
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 2,
  "zenMode.hideLineNumbers": false
}
